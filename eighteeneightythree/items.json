    //CENTER ICONS
    //SPOTIFY
    {
        "type": "appleScriptTitledButton",
        "align": "center",
        "name": "Spotify",
        //"width": 400,
        "image": {
            "base64": ""
        },
        "source": {
            "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rreturn (get artist of current track) & \" – \" & (get name of current track)\rend tell\relse\rreturn \"  \"\rend if\rreturn \"\"\r"
        },
        "refreshInterval": 2,
        "actions": [
            {
                "trigger": "singleTap",
                "action": "appleScript",
                "actionAppleScript": {
                    "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rplaypause\rend tell\rend if\r"
                }
            },
            {
                "trigger": "doubleTap",
                "action": "appleScript",
                "actionAppleScript": {
                    "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rnext track\rend tell\rend if\r"
                }
            },
            {
                "trigger": "tripleTap",
                "action": "appleScript",
                "actionAppleScript": {
                    "inline": "if application \"Spotify\" is running then\rtell application \"Spotify\"\rprevious track\rend tell\rend if\r"
                }
            },
            {
                "trigger": "longTap",
                "action": "appleScript",
                "actionAppleScript": {
                    "inline": "tell application \"Spotify\"\ractivate\rend tell"
                }
            }
        ]
    },
    //RIGHT ICONS
